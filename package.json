    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "husky": "^4.2.5",
    "jest": "^24.1.0",
    "nodemon": "^1.18.9",
